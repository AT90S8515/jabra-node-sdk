{
    "env": {
        "nodeGypCachePath": "${LIBJABRA_NODE_GYP_CACHE_PATH}",
        "cppCompilerPath" : "${LIBJABRA_CPP_COMPILER_PATH}"
    },
    "configurations": [
        {
            "name": "Win32",
            "includePath": [
                "${nodeGypCachePath}",
                "${workspaceFolder}/node_modules/node-addon-api",
                "${workspaceFolder}/includes",
                "${workspaceFolder}/libjabra/headers",
                "${workspaceFolder}/src/main/**"
            ],
            "defines": [
                "_DEBUG",
                "UNICODE",
                "_UNICODE"
            ],
            "windowsSdkVersion": "10.0.17763.0",
            "compilerPath": "${cppCompilerPath}",
            "cStandard": "c11",
            "cppStandard": "c++14",
            "intelliSenseMode": "msvc-x64"
        }
    ],
    "version": 4
}